{
  "name": "mongoose-normalizr",
  "version": "0.0.0-development",
  "description": "Define normalizr entities from mongoose schemas",
  "keywords": [
    "mongoose",
    "normalizr"
  ],
  "homepage": "http://saiichihashimoto.com/mongoose-normalizr/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/mongoose-normalizr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saiichihashimoto/mongoose-normalizr.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --source-maps inline --ignore **/*.spec.js",
    "clean": "rm -rf .nyc_output coverage coverage.lcov lib",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage:test": "cross-env BABEL_ENV=test nyc tape src/**/*.spec.js",
    "lint": "run-s --continue-on-error lint:*",
    "lint:js": "eslint --report-unused-disable-directives --fix --ignore-path .gitignore .",
    "lint:json": "git ls-files | grep \"\\(\\.json$\\)\\|\\(\\..*rc$\\)\" | grep -v package | xargs -n 1 jsonlint --in-place",
    "lint:package-json": "sort-package-json",
    "prepush": "npm test",
    "semantic-release": "semantic-release",
    "test": "tape -r @babel/register src/**/*.spec.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "Install commitizen and use `git cz` instead of `git commit` https://github.com/commitizen/cz-cli#installing-the-command-line-tool"
    }
  },
  "dependencies": {
    "mongoose-legacy-pluralize": "^1.0.2",
    "normalizr": "^3.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "codecov": "^3.0.2",
    "cross-env": "^5.1.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.3.1",
    "husky": "^0.14.3",
    "jsonlint": "github:ginman86/jsonlint#c95cda556d2c719ec723422ac0541a6531356747",
    "lint-staged": "^7.2.0",
    "mongoose": "^5.1.5",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "semantic-release": "^15.5.2",
    "semantic-release-build": "^1.2.0",
    "sort-package-json": "^1.15.0",
    "tape": "^4.9.1",
    "validate-commit-msg": "^2.14.0"
  },
  "peerDependencies": {
    "mongoose": "*"
  },
  "release": {
    "publish": [
      "semantic-release-build",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
