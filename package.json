{
  "name": "mongoose-normalizr",
  "version": "0.0.0-development",
  "description": "Define normalizr entities from mongoose schemas",
  "keywords": [
    "mongoose",
    "normalizr"
  ],
  "homepage": "https://saiichihashimoto.github.io/mongoose-normalizr/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/mongoose-normalizr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saiichihashimoto/mongoose-normalizr.git"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps inline --delete-dir-on-start --verbose --ignore \"**/*.spec.js\"",
    "test": "jest --verbose $(if [ -z \"$CI\" ]; then echo --watch; else echo --color --coverage; fi)"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "dependencies": {
    "mongoose-legacy-pluralize": "1.0.2",
    "normalizr": "3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "codecov": "3.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-saiichihashimoto": "1.1.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-my-app": "3.3.0",
    "mongoose": "5.4.12"
  },
  "peerDependencies": {
    "mongoose": "*"
  }
}
